# ao420fmc.db - DC set, gains, AWG gain and offset. Master
record(ao, "${UUT}:${SITE}:SC32:OFFSET:ALL") {
	field(DTYP, "Soft Channel")        
	field(SCAN, "Passive")
	field(DESC, "Offset ALL")
	field(DRVL, "-2.5")
	field(DRVH, "+2.4999")
	field(LINR, "LINEAR")
	field(ESLO, "76.2939e-6")            # 5/65535
	field(EOFF, "2.5")
	field(HOPR, "2.4999")
	field(LOPR, "-2.5")
	field(EGU,  "V")
	field(FLNK, "${UUT}:${SITE}:SC32:OFFSET:fan")
}

record(dfanout, "${UUT}:${SITE}:SC32:OFFSET:fan") {
	field(OUTA, "${UUT}:${SITE}:SC32:OFFSET:fan1 PP")
	field(OUTB, "${UUT}:${SITE}:SC32:OFFSET:fan2 PP")
	field(OUTC, "${UUT}:${SITE}:SC32:OFFSET:fan3 PP")
	field(OUTD, "${UUT}:${SITE}:SC32:OFFSET:fan4 PP")
	field(DOL,  "${UUT}:${SITE}:SC32:OFFSET:ALL")
	field(OMSL, "closed_loop")
}

